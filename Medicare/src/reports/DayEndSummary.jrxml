	<queryString>
		<![CDATA[SELECT
    (@row:=@row+1) AS ROW,
     count(*) AS no_of_patients,
     ((svc.`CHARGES`*count(*)) - act.`ZAKAT`) AS cash,
     0 AS Donation,
     0 AS Other,
     (svc.`CHARGES`*count(*)) AS Total,
     svc.`SVC_NAME` AS service_name,
     svc.`CHARGES` AS rate,
     act.`ZAKAT` AS zakat,
     act.`VISIT_DATE` AS Date
FROM
     activities act ,services svc,(SELECT @row := 0) r 

WHERE
      act.`SERVICE_ID` = svc.`SERVICE_ID` and act.`VISIT_DATE` =$P{date}
GROUP BY
     svc.`SVC_NAME`
ORDER BY
     ROW ASC]]>
	</queryString>
